<div class="form-group">
  <%= f.label :name %>
	<%= f.text_field :name, :class => "form-control" %>
</div>
<div class="form-group">
	<%= f.file_field :photo, :class => "form-control" %>
</div>
<div class="form-group">
	<%= f.label :description %>
	<%= f.text_area :description, :class => "form-control" %>
</div>
<div class="form-group">
	<%= f.label :skype_id %>
	<%= f.text_field :skype_id, :class => "form-control" %>
</div>
<div class="form-group">
	<%= f.label :price, "price per hour" %>
	<%= f.text_field :price, :class => "form-control" %>
</div>
<div class="form-group">
  <%= f.label :availabletime_ids, "availabletime" %>
	<%= f.select :availabletime_ids, Availabletime.all.map{ |x| [ x.time_start, x.id] }, {}, :multiple => true, :class => "form-control" %>
</div>
	<%= f.submit :class => "btn btn-default"%>